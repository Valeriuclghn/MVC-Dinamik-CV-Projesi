
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Yetenek Ekleme Sayfası</h4>
<br />
<form method="post">
    <input type="text" name="Baslık" class="form-control"  placeholder="Başlığı Yazınız"/>
    <br />
    <input type="text" name="AltBaslık" class="form-control"  placeholder="Alt Başlık"/>
    <br />
    <input type="text" name="Tarih" class="form-control"  placeholder="Çalışma Tarihi"/>
    <br />
    <textarea placeholder="Açıklmanızı giriniz" name="Açıklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

